{
  "type": "apoli:multiple",
  "resource": {
    "type": "apoli:resource",
    "min": 0,
    "max": 1
  },
  "toggle": {
    "type": "apoli:active_self",
    "cooldown": 10,
    "entity_action": {
      "type": "apoli:if_else",
      "condition": {
        "type": "apoli:resource",
        "resource": "*:*_resource",
        "comparison": "==",
        "compare_to": 1
      },
      "if_action": {
        "type": "apoli:and",
        "actions": [
          {
            "type": "apoli:change_resource",
            "resource": "*:*_resource",
            "operation": "set",
            "change": 0
          },
          {
            "type": "apoli:add_velocity",
            "y": 0,
            "set": true
          },
          {
            "type": "apoli:play_sound",
            "sound": "touhouorigins:entity.amanojaku.flip"
          }
        ]
      },
      "else_action": {
        "type": "apoli:and",
        "actions": [
          {
            "type": "apoli:change_resource",
            "resource": "*:*_resource",
            "operation": "set",
            "change": 1
          },
          {
            "type": "apoli:play_sound",
            "sound": "touhouorigins:entity.amanojaku.flip"
          }
        ]
      }
    },
    "condition": {
      "type": "apoli:sneaking"
    }
  },
  "flip": {
    "type": "touhouorigins:flip_model",
    "condition": {
      "type": "apoli:resource",
      "resource": "*:*_resource",
      "comparison": "==",
      "compare_to": 1
    }
  },
  "reverse_y": {
    "type": "apoli:modify_velocity",
    "axes": [
      "y"
    ],
    "modifier": {
      "value": -1.1,
      "operation": "multiply_total_multiplicative"
    },
    "condition": {
      "type": "apoli:resource",
      "resource": "*:*_resource",
      "comparison": "==",
      "compare_to": 1
    }
  }
}